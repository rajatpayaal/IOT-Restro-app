<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.iot.iotrestaurantapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx"
        android:icon="@mipmap/logooo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:appComponentFactory"
        tools:targetApi="p">
        <activity android:name="com.iot.iotrestaurantapp.view.home.control" />
        <activity android:name="com.iot.iotrestaurantapp.view.home.iot1" />
        <activity android:name="com.iot.iotrestaurantapp.view.home.login" />
        <activity android:name="com.iot.iotrestaurantapp.view.home.Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.iot.iotrestaurantapp.view.home.MainActivity" />
        <activity
            android:name="com.iot.iotrestaurantapp.view.home.HomeActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.iot.iotrestaurantapp.view.category.CategoryActivity"
            android:label="Category"
            android:screenOrientation="fullSensor"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.iot.iotrestaurantapp.view.detail.DetailActivity"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>